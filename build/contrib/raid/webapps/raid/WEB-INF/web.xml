<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">


    <servlet>
        <servlet-name>org.apache.hadoop.mapred.jobmonitor_jsp</servlet-name>
        <servlet-class>org.apache.hadoop.mapred.jobmonitor_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.hadoop.mapred.raid_jsp</servlet-name>
        <servlet-class>org.apache.hadoop.mapred.raid_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.hadoop.mapred.blockfixer_jsp</servlet-name>
        <servlet-class>org.apache.hadoop.mapred.blockfixer_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.hadoop.mapred.raidfsck_jsp</servlet-name>
        <servlet-class>org.apache.hadoop.mapred.raidfsck_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.hadoop.mapred.jobmonitor_jsp</servlet-name>
        <url-pattern>/jobmonitor.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.hadoop.mapred.raid_jsp</servlet-name>
        <url-pattern>/raid.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.hadoop.mapred.blockfixer_jsp</servlet-name>
        <url-pattern>/blockfixer.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.hadoop.mapred.raidfsck_jsp</servlet-name>
        <url-pattern>/raidfsck.jsp</url-pattern>
    </servlet-mapping>

</web-app>

